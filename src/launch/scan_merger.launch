<launch>
	
	<node pkg="ira_laser_tools" name="laserscan_multi_merger" type="laserscan_multi_merger" output="screen">
	<param name="destination_frame" value="base_link"/>
	<param name="cloud_destination_topic" value="/merged_cloud"/>
	<param name="scan_destination_topic" value="/scan_multi"/>
	<param name="laserscan_topics" value ="/front/scan /rear/scan" /> 
	<param name="angle_min" value="-3.14"/>
	<param name="angle_max" value="3.14"/>
	<param name="angle_increment" value="0.0058"/>
	<param name="scan_time" value="0.0333333"/>
	<param name="range_min" value="0.50"/>
	<param name="range_max" value="50.0"/>
	</node>


	<node pkg="tf2_ros" type="static_transform_publisher" name="front" args="0.35 0.0 0.0 0.0 0.0 0.5150381 0.8571673 base_link front" />

	<node pkg="tf2_ros" type="static_transform_publisher" name="rear" args="-0.35 0.0 0.0 0.0 0.0 0.866 0.5 base_link rear" />
	



</launch>
